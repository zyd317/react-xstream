{
  "name": "react-xstream",
  "version": "1.0.0",
  "description": "基于[xstream](https://github.com/staltz/xstream)封装的react响应式状态管理解决方案",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "publish": "webpack --mode production",
    "analyzer": "webpack --mode production --env analyzer",
    "fix": "eslint --fix"
  },
  "files": [
    "esm/",
    "lib/"
  ],
  "lint-staged": {
    "linters": {
      "*.{ts,tsx}": [
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": [
      "build",
      "test",
      "log"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:zyd317/react-xstream.git"
  },
  "keywords": [
    "react-xstream",
    "react-xstream-store",
    "react store",
    "react model"
  ],
  "author": "xiaochan(1427182960@qq.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zyd317/react-xstream/issues"
  },
  "homepage": "https://github.com/zyd317/react-xstream#readme",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tslib": "^1.9.3",
    "typescript": "^3.3.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "xstream": "^11.10.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "autoprefixer": "^9.5.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-ttdefault": "^1.0.7",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  }
}
